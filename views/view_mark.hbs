<div class="container-fluid">
    <div class="row">
        <!-- Sidebar -->
        <nav id="sidebar" class="col-md-3 col-lg-2 d-md-block bg-light sidebar min-vh-100 ">
            <div class="position-sticky">
                <ul class="nav flex-column">
                    <li class="nav-item">
                        <a class="nav-link active" href="/admin/r_teacher">
                            New register Teacher
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/admin/r_student">
                            New register Student
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/admin/new_project">
                            New Project
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" id="loadProject" href="/admin/view_teacher">
                            Manage Teacher
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/admin/view_student">
                            Manage Student
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/admin/view_project">
                            Manage Project
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/admin/view_group">
                            Manage Group
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/admin/view_mark">
                            Manage Mark
                        </a>
                    </li>
                </ul>
            </div>
        </nav>

        <!-- Main Content -->
        <main class="col-md-9 ms-sm-auto col-lg-10 px-md-4" style="background-color: #DDE6ED">
            <div class="heading d-flex justify-content-between align-items-center">
                <div class=" h1 text-primary p-4">Manage Marks</div>
                <div>
                    <form class="form-inline my-2 my-lg-0" action="/admin/view_mark" method="get">
                        <input class="form-control mr-sm-2" type="text" placeholder="Search Name" aria-label="Search"
                            name="search">
                        <button class="btn btn-outline-success my-2 my-sm-0" type="submit">Search</button>
                    </form>
                </div>
            </div>

            <div class="table-responsive">
                <table class="table table-striped">
                    <thead>
                        <tr>
                            <th scope="col">Project Id</th>
                            <th scope="col">Group Id</th>
                            <th scope="col">Student Name</th>
                            <th scope="col">Teacher ID</th>
                            <th scope="col">File 1</th>
                            <th scope="col">Mark 1</th>
                            <th scope="col">Status 1</th>
                            <th scope="col">File 2</th>
                            <th scope="col">Mark 2</th>
                            <th scope="col">Status 2</th>
                            <th scope="col">File 3</th>
                            <th scope="col">Mark 3</th>
                            <th scope="col">Status 3</th>
                            <th scope="col">File 4</th>
                            <th scope="col">Mark 4</th>
                            <th scope="col">Status 4</th>
                            <th scope="col">Total</th>
                            <th scope="col">Year</th>
                            <th scope="col">Action</th>
                        </tr>
                    </thead>
                    <tbody>
                        {{#each result}}
                        <tr>
                            <td>{{this.p_id}}</td>
                            <td>{{this.group_id}}</td>
                            <td>{{this.st_name}}</td>
                            <td>{{this.t_id}}</td>
                            <td>{{this.file1}}</td>
                            <td>{{this.level1}}</td>
                            <td>{{this.status1}}</td>
                            <td>{{this.file2}}</td>
                            <td>{{this.level2}}</td>
                            <td>{{this.status2}}</td>
                            <td>{{this.file3}}</td>
                            <td>{{this.level3}}</td>
                            <td>{{this.status3}}</td>
                            <td>{{this.file4}}</td>
                            <td>{{this.level4}}</td>
                            <td>{{this.status4}}</td>
                            <td>{{this.total}}</td>
                            <td>{{this.year}}</td>
                            <td>
                                <!-- <a href="/edit_student/{{this.p_id}}" type="button"
                            class="btn btn-light btn-small"><i class="bi bi-pencil-fill"></i>Edit</a> -->
                                <a href="/delete_mark/{{this.p_id}}" type="button" class="btn btn-danger btn-small"><i
                                        class="bi bi-trash3-fill"></i>Delete</a>
                            </td>
                        </tr>
                        {{/each}}
                    </tbody>
                </table>
            </div>
        </main>



    </div>

</div>