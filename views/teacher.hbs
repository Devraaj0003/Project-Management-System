
<div class="container-fluid">
  <div class="row">
    <!-- Sidebar -->
    <nav id="sidebar" class="col-md-3 col-lg-2 d-md-block bg-light sidebar min-vh-100 ">
   
      <div class="position-sticky">
        <ul class="nav flex-column">
          <li class="nav-item">
            <a class="nav-link active" href="/teacher">
              Dashboard
            </a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="/teacher/view_mygroups/{{t_id}}">
              View Groups
            </a>
          </li>
          
        </ul>
        
      </div>
    </nav>

    <!-- Main Content -->
    <main class="col-md-9 ms-sm-auto col-lg-10 px-md-4">
         <div class="display-5 text-secondary">
          Welcome {{userName}}
        </div>
     <h1 class="text-primary p-4">Dashboard  </h1>
      <!-- Your page content goes here -->
        <div class="table-responsive">
                <table class="table table-striped table-bordered">
                    <thead class="thead-dark">
                        <tr>
                            <th scope="col">Id</th>
                            <th scope="col">Group Id</th>
                            <th scope="col">Project Name</th>
                            <th scope="col">Project Detail</th>
                            <th scope="col">Student Id</th>
                            <th scope="col">Student Name</th>
                            <th scope="col">Teacher Name</th>
                            <th scope="col">Year</th>
                            <th scope="col">Type</th>
                            <th scope="col">Start Date</th>
                            <th scope="col">End Date</th>
                            <th scope="col">Status</th>
                            <th scope="col">Action</th>
                        </tr>
                    </thead>
                    <tbody>
                        {{#each result}}
                        <tr>
                            <td>{{this.p_id}}</td>
                            <td>{{this.group_id}}</td>
                            <td>{{this.project_name}}</td>
                            <td>{{this.project_detail}}</td>
                            <td>{{this.s_id}}</td>
                            <td>{{this.student_name}}</td>
                            <td>{{this.teacher_name}}</td>
                            <td>{{this.year}}</td>
                            <td>{{this.type}}</td>
                            <td>{{this.start_date}}</td>
                            <td>{{this.end_date}}</td>
                            <td>{{this.status}}</td>
                            <td>
                                <a href="/view_studproject/{{this.p_id}}" type="button" class="btn btn-success btn-sm"><i
                                        class="bi bi-pencil-fill"></i>view</a>
                                {{!-- <a href="/delete_project/{{this.p_id}}" type="button"
                                    class="btn btn-danger btn-sm"><i class="bi bi-trash3-fill"></i>Delete</a>
                                --}}
                            </td>
                        </tr>
                        {{/each}}
                    </tbody>
                </table>
                {{noresult}}
            </div>
    </main>
  </div>
</div>